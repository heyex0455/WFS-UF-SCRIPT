-- Tab for fun activities
local Tab = Window:CreateTab("fun activityüß∏", 4483362458)

local Button = Tab:CreateButton({
    Name = "Long arms | MUST HAVE GOOD PING",
    Callback = function()
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()

        local function stretchHands(character)
            local leftHand = character:WaitForChild("LeftHand")
            local rightHand = character:WaitForChild("RightHand")

            leftHand.Size = Vector3.new(1, 35, 1)
            rightHand.Size = Vector3.new(1, 35, 1)

            leftHand.CanCollide = false
            rightHand.CanCollide = false
        end

        local function detectAndGrabBall()
            local ballGrabRange = 50
            local ball = game.Workspace:FindFirstChild("Football")
            if ball then
                local distance = (ball.Position - character.HumanoidRootPart.Position).magnitude
                if distance <= ballGrabRange then
                    ball.CFrame = character.HumanoidRootPart.CFrame * CFrame.new(0, 0, -5)
                    local sound = Instance.new("Sound", ball)
                    sound.SoundId = "rbxassetid://123456789"
                    sound:Play()
                end
            end
        end

        stretchHands(character)

        local runService = game:GetService("RunService")
        runService.Heartbeat:Connect(function()
            detectAndGrabBall()
        end)
    end,
})
