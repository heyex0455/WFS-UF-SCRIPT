-- Set up your webhook URL here
local webhookURL = "https://discord.com/api/webhooks/1306506599040290846/uw3l6F1CaLWEgTChFaxXSdzf13u56HQ4MkGL2_-gHs2dtfBP9y3dnXAX1ODjgZ0mmpd5"

-- Function to send log message to Discord
local function sendLogToDiscord(message)
    local data = {
        ["content"] = message
    }
    
    local headers = {
        ["Content-Type"] = "application/json"
    }

    local httpService = game:GetService("HttpService")
    local jsonData = httpService:JSONEncode(data)

    -- Send HTTP request to the webhook
    local success, response = pcall(function()
        return game:GetService("HttpService"):PostAsync(webhookURL, jsonData, Enum.HttpContentType.ApplicationJson)
    end)

    -- Check if the request was successful
    if not success then
        warn("Failed to send log to Discord:", response)
    end
end

-- Log message when script is executed
local playerName = game.Players.LocalPlayer.Name
local timeExecuted = os.date("%Y-%m-%d %H:%M:%S")  -- Get the current time
local logMessage = string.format("Script executed by: %s | Time: %s", playerName, timeExecuted)

-- Send log to Discord
sendLogToDiscord(logMessage)

